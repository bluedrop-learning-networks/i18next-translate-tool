{
  "name": "translation-tool",
  "version": "1.0.0",
  "description": "CLI tool for translating i18next locale files using AI",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "node --test dist/**/*.spec.js",
    "lint": "biome lint .",
    "format": "biome format . --write",
    "check": "biome check --apply ."
  },
  "keywords": ["i18next", "translation", "cli", "ai"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@types/node": "^20.10.5",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "cmd-ts": "^0.13.0",
    "i18next": "^23.7.11"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@8.6.0"
}
